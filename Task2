import random


def get_numbers_ticket(a, b, c):
    numbers = []
    for i in range(a, b+1, 1):
        numbers.append(i) 
    lnumbers = random.sample(numbers, c)
    lnumbers.sort()
    return  lnumbers


lottery_numbers = get_numbers_ticket(1, 49, 6)
print("Ваші лотерейні числа:", lottery_numbers)
